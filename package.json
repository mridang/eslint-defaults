    "@semantic-release/release-notes-generator": "^14.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "dotenv": "^17.2.3",
    "jest": "^30.0.3",
    "knip": "^5.58.0",
    "prettier": "^3.5.3",
    "semantic-release": "^25.0.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@eslint/js": "^9.27.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
