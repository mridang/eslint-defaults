{
  "name": "@mridang/eslint-defaults",
  "scripts": {
    "prepack": "tsc",
    "build": "tsc",
    "test": "jest --verbose --config=jest.config.js --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:debug": "jest --verbose --config=jest.config.js --runInBand --detectOpenHandles",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": {
    "name": "Mridang Agarwalla",
    "email": "mridang.agarwalla@gmail.com",
    "url": "https://github.com/mridang"
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "description": "A set of sane default for the NestJS framework",
  "private": false,
  "peerDependencies": {
    "eslint": ">=9"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mridang/eslint-defaults.git"
  },
  "homepage": "https://github.com/mridang/eslint-defaults",
